{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning",
      "Newmark.API.Repositories.OptimizedBlobPropertyRepository": "Information",
      "Microsoft.Extensions.Caching.Memory": "Warning"
    }
  },
  "AzureBlob": {
    "BlobUrl": "https://nmrkpidev.blob.core.windows.net/dev-test/devtest.json",
    "SasToken": "YOUR_SAS_TOKEN_HERE",
    "MaxDownloadSizeMB": 200,
    "DownloadTimeoutMinutes": 10
  },
  "Caching": {
    "MaxCacheSizeMB": 100,
    "CacheExpiryMinutes": 30
  },
  "Kestrel": {
    "MaxRequestSizeMB": 250,
    "KeepAliveTimeoutMinutes": 15,
    "RequestTimeoutMinutes": 5,
    "MaxConnections": 1000,
    "MaxUpgradedConnections": 1000
  },
  "AllowedHosts": "*"
}
